<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE manifest [
    <!ENTITY CATEGORY_NAME "org.afree.chart.demo">
]>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	android:versionCode="1" android:versionName="1.0" package="org.moca">
	<uses-sdk android:minSdkVersion="3" />
	<application android:icon="@drawable/icon" android:label="@string/app_name"
		android:debuggable="true">

		<service android:name=".service.BackgroundUploader"
			android:icon="@drawable/icon" android:label="@string/title_upload_service"
			android:enabled="true" />

		<service android:name="org.moca.activity.FunctionEval" /> 
        <service android:name="org.moca.service.CorscienceService" android:process=":remote">
        	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
        </service>
        <service android:name="org.moca.service.SimpleService" android:process=":remote">
        <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
        </service>
		<provider android:name=".db.ProcedureProvider"
			android:authorities="org.moca.provider.Procedure" />

		<provider android:name=".db.SavedProcedureProvider"
			android:authorities="org.moca.provider.SavedProcedure" />

		<provider android:name=".db.ImageProvider"
			android:authorities="org.moca.provider.Image" />

		<provider android:name=".db.SoundProvider"
			android:authorities="org.moca.provider.Sound" />

		<provider android:name=".db.BinaryProvider"
			android:authorities="org.moca.provider.Binary" />
		
		<provider android:name=".db.FUNCEVALProvider"
			android:authorities="org.moca.provider.FUNCEVAL" />

		<provider android:name=".db.NotificationProvider"
			android:authorities="org.moca.provider.Notification" />

		<provider android:name=".db.PatientProvider"
			android:authorities="org.moca.provider.Patient" />

		<provider android:name=".db.EventProvider"
			android:authorities="org.moca.provider.Event" />

		<activity android:name=".activity.ProceduresList"
			android:label="@string/title_procedures_list"
			android:screenOrientation="portrait">
			<intent-filter>
				<action android:name="android.intent.action.VIEW" />
				<action android:name="android.intent.action.PICK" />
				<category android:name="android.intent.category.DEFAULT" />
				<data android:mimeType="vnd.android.cursor.dir/org.moca.procedure" />
			</intent-filter>
			<intent-filter>
				<action android:name="android.intent.action.GET_CONTENT" />
				<category android:name="android.intent.category.DEFAULT" />
				<data android:mimeType="vnd.android.cursor.item/org.moca.procedure" />
			</intent-filter>
		</activity>
		<activity android:name=".activity.SavedProcedureList"
			android:label="@string/title_savedprocedure_list"
			android:screenOrientation="portrait">
			<intent-filter>
				<action android:name="android.intent.action.VIEW" />
				<action android:name="android.intent.action.PICK" />
				<category android:name="android.intent.category.DEFAULT" />
				<data android:mimeType="vnd.android.cursor.dir/org.moca.savedProcedure" />
			</intent-filter>
			<intent-filter>
				<action android:name="android.intent.action.GET_CONTENT" />
				<category android:name="android.intent.category.DEFAULT" />
				<data android:mimeType="vnd.android.cursor.item/org.moca.savedProcedure" />
			</intent-filter>
		</activity>
		<activity
            android:name="InfoActivity">
            </activity>
        <activity
            android:name=".activity.FunctionEval">
         </activity>
         <activity android:name="org.afree.chart.demo.activity.DeviationRendererDemo02Activity"
            android:label="Deviation Renderer Demo 02"
            android:configChanges="orientation|keyboardHidden">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="&CATEGORY_NAME;" />
            </intent-filter>
        </activity>
		<activity android:name=".activity.NotificationList"
			android:label="@string/title_notifications_list"
			android:screenOrientation="portrait">
			<intent-filter>
				<action android:name="android.intent.action.VIEW" />
				<action android:name="android.intent.action.PICK" />
				<category android:name="android.intent.category.DEFAULT" />
				<data android:mimeType="vnd.android.cursor.dir/org.moca.notification" />
			</intent-filter>
			<intent-filter>
				<action android:name="android.intent.action.GET_CONTENT" />
				<category android:name="android.intent.category.DEFAULT" />
				<data android:mimeType="vnd.android.cursor.item/org.moca.notification" />
			</intent-filter>
		</activity>
		<activity android:name=".activity.ProcedureRunner"
			android:label="@string/title_procedure_runner"
			android:screenOrientation="portrait">
			<intent-filter>
				<action android:name="android.intent.action.VIEW" />
				<category android:name="android.intent.category.DEFAULT" />
				<data android:mimeType="vnd.android.cursor.item/org.moca.procedure" />
			</intent-filter>
			<intent-filter>
				<action android:name="android.intent.action.VIEW" />
				<category android:name="android.intent.category.DEFAULT" />
				<data android:mimeType="vnd.android.cursor.item/org.moca.savedProcedure" />
			</intent-filter>
		</activity>
		<activity android:name=".activity.NotificationViewer"
			android:label="@string/title_notification_viewer"
			android:screenOrientation="portrait">
			<intent-filter>
				<action android:name="android.intent.action.VIEW" />
				<category android:name="android.intent.category.DEFAULT" />
				<data android:mimeType="vnd.android.cursor.item/org.moca.notification" />
			</intent-filter>
		</activity>
		<activity android:name=".activity.Settings" android:label="@string/title_settings"
			android:screenOrientation="portrait">
			<intent-filter>
				<action android:name="android.intent.action.VIEW" />
				<category android:name="android.intent.category.DEFAULT" />
				<data android:mimeType="vnd.android.cursor.item/org.moca.settings" />
			</intent-filter>
		</activity>
		<activity android:name=".activity.Moca" android:label="@string/app_name"
			android:screenOrientation="portrait">
			 
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
			
		</activity>
		<activity android:name="activity.PlotResults" android:label="@string/title_capture_activity"
			android:screenOrientation="landscape">
			<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
			<uses-permission android:name="android.permission.READ_OWNER_DATA"></uses-permission>
			<!--
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter> -->
		</activity>
		<activity android:name=".activity.Capture" android:label="@string/title_capture_activity"
			android:screenOrientation="landscape">
		</activity>
		<receiver android:name=".net.SMSReceive">
			<intent-filter>
				<action android:name="android.provider.Telephony.SMS_RECEIVED" />
			</intent-filter>
		</receiver>
		<activity android:name=".activity.FunctionEval" android:permission="android.permission.WRITE_EXTERNAL_STORAGE"></activity>
	</application>
	<uses-permission xmlns:android="http://schemas.android.com/apk/res/android"
		android:name="android.permission.INTERNET"></uses-permission>
	<uses-permission xmlns:android="http://schemas.android.com/apk/res/android"
		android:name="android.permission.RECEIVE_SMS"></uses-permission>
	<uses-permission xmlns:android="http://schemas.android.com/apk/res/android"
		android:name="android.permission.CAMERA"></uses-permission>
	<uses-permission xmlns:android="http://schemas.android.com/apk/res/android"
		android:name="android.permission.RECORD_AUDIO"></uses-permission>
	<uses-permission xmlns:android="http://schemas.android.com/apk/res/android"
		android:name="android.permission.VIBRATE"></uses-permission>
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
	<uses-permission android:name="android.permission.READ_OWNER_DATA"></uses-permission>
	<uses-permission android:name="android.permission.ACCESS_PHONE_SUB_INFO" />
	<uses-permission android:name="android.permission.READ_PHONE_STATE"></uses-permission>
	<uses-permission android:name="android.permission.ACCESS_WIFI_STATE"></uses-permission>
	<uses-permission android:name="android.permission.CHANGE_WIFI_STATE"></uses-permission>
	<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"></uses-permission>
	<uses-permission android:name="android.permission.BLUETOOTH"></uses-permission>
	<uses-permission android:name="android.permission.BLUETOOTH_ADMIN"></uses-permission>
</manifest>
