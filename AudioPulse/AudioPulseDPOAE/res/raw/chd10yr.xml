<Procedure title="DPOAE Hearing Test" author="Sana Team">
	<Page>
		<Element type="ENTRY" id="age" concept="Age" question="Gestational Age (weeks)"
		numeric="DIALPAD" required="true" helpText="Gestational Age (weeks)" answer="" />
	</Page>
	<Page>
		<Element type="RADIO" id="gender" concept="Gender" question="Gender"
		required="true" helpText="Patient's gender" answer="" choices="male, female" />
	</Page>
	<Page>
		<Element type="MULTI_SELECT" id="risk" question="Patient Risk Factors"
			helpText="Select patient's risk factors" answer="" choices="family history, hospital stay longer than 5 days, in-utero infections, ototoxic drugs, syndromes related to hearing loss" />
	</Page>
	<Page>
		<Element type="FUNCEVAL" concept="execute" id="chd10yr"
			content-type="application/vnd.sensor.ecg" question="Press button to measure hearing" />
	</Page>
</Procedure>
